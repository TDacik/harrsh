# Not every tree is almost linear
query {
  tree(x1, x2) |= ltree(x1, x2)
}
sid {
  tree <= x1 -> (nil, nil) ;
  tree <= x1 -> (y,z) * tree(y) * tree(z) ;
  # Every right child of an ltree is a leaf => Almost linear trees
  ltree <= x1 -> (nil, nil) ;
  ltree <= x1 -> (y,z) * ltree(y) * rtree(z) ;
  rtree <= x1 -> (nil, nil) ;
}
info {
  status = false
}
