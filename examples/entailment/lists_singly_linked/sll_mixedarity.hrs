# All lists are mixedarity-lists, because we can always pick the second rule
query {
  sll(x1, x2) |= mixedarity(x1, x2)
}
sid {
  sll <= x1 -> x2 ;
  sll <= x1 -> y * sll(y,x2) ;
  mixedarity <= x1 -> (y,nil) * mixedarity(y, x2);
  mixedarity <= x1 -> y * mixedarity(y, x2);
  mixedarity <= x1 -> (x2,nil) ;
  mixedarity <= x1 -> x2 ;
}
info {
  status = true
}